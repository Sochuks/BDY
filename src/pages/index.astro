<style>
  .border-overlay {
    position: absolute;
    left: 0;
    width: 100%;
    border-bottom: 8px solid var(--color-primary-gold);
    border-left: 8px solid var(--color-primary-gold);
    border-right: 8px solid var(--color-primary-gold);
    z-index: 30; /* In front of image */
    background: transparent;
  }

 /* Shiny line overlay effect */
.border-box::before {
  content: "";
  position: absolute;
  top: -50%;
  left: -50%;
  width: 200%;
  height: 200%;
  background: linear-gradient(
    120deg,
    rgba(255, 255, 255, 0) 30%,
    rgba(255, 255, 255, 0.6) 50%,
    rgba(255, 255, 255, 0) 70%
  );
  transform: rotate(0deg);
  animation: shine 2.5s infinite ease-in-out;
  z-index: 20;
  pointer-events: none;
  filter: blur(2px);
}

@keyframes shine {
  0% {
    transform: translateX(-100%) translateY(-100%) rotate(25deg);
  }
  50% {
    transform: translateX(100%) translateY(100%) rotate(25deg);
  }
  100% {
